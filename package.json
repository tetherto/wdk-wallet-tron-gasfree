{
  "name": "@wdk/wallet-tron-gasfree",
  "version": "1.0.0-beta.1",
  "description": "A simple package to manage BIP-32 wallets for the tron blockchain with gasfree features.",
  "keywords": ["wdk", "wallet", "bip-32", "tron", "gasfree"],
  "author": "Tether",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/tetherto/wdk-wallet-tron-gasfree.git"
  },
  "main": "index.js",
  "type": "module",
  "types": "./types",
  "scripts": {
    "build:types": "tsc",
    "lint": "standard",
    "lint:fix": "standard --fix --env jest"
  },
  "dependencies": {
    "@noble/curves": "1.9.2",
    "@noble/hashes": "1.8.0",
    "@noble/secp256k1": "2.3.0",
    "@wdk/wallet": "https://github.com/davi0kprogramsthings/wdk-wallet.git#develop",
    "@wdk/wallet-tron": "https://github.com/davi0kprogramsthings/wdk-wallet-tron.git#develop",
    "bare-node-runtime": "1.0.3",
    "tronweb": "5.3.4"
  },
  "devDependencies": {
    "standard": "17.1.2",
    "typescript": "5.8.3"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "bare": "./bare.js",
      "default": "./index.js"
    },
    "./package": {
      "default": "./package.json"
    }
  },
  "standard": {
    "ignore": [
      "bare.js",
      "tests/**/*.js"
    ]
  }
}
